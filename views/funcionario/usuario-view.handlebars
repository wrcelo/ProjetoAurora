        </ul>




    </div>
  </header>
<hr style="border: 1.5px solid #c1cd30;" class="">
<section id="table-box" class="d-flex flex-column container py-5">
    <div class="mb-4 d-flex justify-content-between align-items-center">
<div>

        <h1 class="m-0 fs-1">Associados</h1>
        <span class="lead fs-5">Consulte os associados abaixo</span>
</div>
        
  <div>
    <a href="/menu/funcionario/1/visualizar" class="text-dark text-decoration-none">Resetar Filtros</a>
  </div>


    </div>
    <table class="table w-100 p-3 fs-5 text-center">
      <thead class="table-dark">
        <tr >
          <th scope="col" >ID</th>
          <th scope="col" >Nome</th>
          <th scope="col" >Email</th>
          <th  scope="col">CPF</th>
          <th scope="col" >Data Nascimento</th>
          <th scope="col" >Status</th>
          <th scope="col" >Opções</th>
        </tr>
      </thead>
        {{#each listaUsuarios}}
        <tr>
            <td>{{this.id}}</td>
            <td>{{this.nome_usuario}}</td>
            <td>{{this.email_usuario}}</td>
            <td>{{this.cpf_usuario}}</td>
            <td>{{this.data_nascimento_usuario}}</td>
            <td>{{this.[Associado.status]}}</td>


              
            <td style="min-height:44px;" class="d-flex justify-content-between">
              <form  class="d-flex align-items-center m-0" action="/menu/funcionario/aprovar/func={{this.funcionario}}&user={{this.id}}" method="post">
                <button class="link-primary" type="submit" style="background-color: transparent; color: #009ec5; text-decoration:underline; border:none;">
                  <i class="bi bi-toggles"></i>
                </button>
              </form>
              <a style="color: #009ec5"class=" text-decoration-none d-flex align-items-center gap-2" href="/dependentes/funcionario/user={{this.id}}&func={{this.funcionario}}">
                <i class="bi bi-people-fill"></i>
              </a>
              <a style="color: #009ec5"class=" text-decoration-none d-flex align-items-center gap-2" href="/menu/funcionario/{{this.funcionario}}/editar/{{this.id}}"><i class="bi bi-pencil-square"></i>
              </a>
              <a style="color: #009ec5"class=" text-decoration-none d-flex align-items-center gap-2" href="/menu/funcionario/historico/func={{this.funcionario}}&user={{this.id}}" class="link-primary">
              <i class="bi bi-clock-history"></i>
              </a><a  style="color: #009ec5"class=" text-decoration-none d-flex align-items-center gap-2 text-danger" href="/usuarios/delete/func={{this.funcionario}}&user={{this.id}}"><i class="bi bi-trash-fill"></i></a></td>
            
        </tr>
        {{/each}}
      
    </table>
    </section>