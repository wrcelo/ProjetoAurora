  <header style="background-color: #0095BA;" class="d-flex p-2">
    <div class="container">



        <ul class="nav nav-pills d-flex justify-content-between gap-3">    
          <div class="d-flex align-items-center">
            
            <li class="nav-item">
              <a href="/menu/funcionario/{{idFunc}}" class=" text-dark fs-4" aria-current="page">
                <i class="bi bi-chevron-left text-light"></i>
              </a></li>
          </div>
          <div class="d-flex ">
            
            <li class="nav-item align-items-center d-flex">

                  <form class="input-group m-0 h-100" action="/menu/funcionario/{{idFunc}}/visualizar/buscar" method="GET">
                  <input type="search" class="form-control" name="busca" placeholder="Filtrar por associado">
                  <button style="background-color: #009ec5;" type="submit" class="btn shadow-sm text-light d-flex align-items-center">
                   <i class="bi bi-search"></i>
                  </button>

                  </form>

              </li>

          <li class="nav-item"><a  href="/menu/funcionario/{{idFunc}}/reservas" class="nav-link d-flex align-items-center gap-2 text-light"  aria-current="page"><i class="bi bi-calendar-week"></i>Reservas</a></li>
          <li class="nav-item"><a style="background-color: #009ec5;" href="/menu/funcionario/{{idFunc}}/visualizar" class="shadow-sm nav-link d-flex align-items-center gap-2 text-light" aria-current="page"><i class="bi bi-person-lines-fill"></i>Associados</a></li>
          <li class="nav-item"><a href="/login/logout" class="nav-link d-flex align-items-center gap-2 text-light"><i class="bi bi-box-arrow-left"></i>Sair</a></li>
